#!groovy

@Library("Infrastructure@camunda-only-pipeline")

def type = "java"
def product = "ia"
def component = "task-configuration"
def s2sServiceName = "wa_task_configuration_api"

withCamundaOnlyPipeline(type, product, component, s2sServiceName) {

  after('test') {
    steps.archiveArtifacts allowEmptyArchive: true, artifacts: '**/reports/tests/**/*'
    steps.archiveArtifacts allowEmptyArchive: true, artifacts: '**/reports/tests/integration/**/*'
  }

}
